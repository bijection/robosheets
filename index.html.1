                                <script type="text/javascript">
                                    require(['foundation/main'], function(main) {
                                        require([
                                                'jquery/nyt',
                                                'underscore/nyt',
                                                'foundation/models/user-data',
                                                'foundation/views/base-view',
                                                'shared/sharetools/instances/cross-platform-save',
                                                'shared/data/collections/recommendations'
                                            ],
                                            function($, _, userData, BaseView, ReadingList, Recommendations) {
                                                var MyStoriesView = BaseView.extend({

                                                    savedItemsCollection: null,

                                                    recommendedCollection: null,

                                                    totalSavedItems: null,

                                                    renderableSavedStories: [],

                                                    events: {
                                                        'click a': 'handleLinkClick',
                                                        'click .save-action': 'handleSaveToggleClick'
                                                    },

                                                    variant: null,

                                                    initialize: function() {
                                                        this.$el = $('#my-stories');
                                                        this.setupImpressionTracking = _.once(this._setupImpressionTracking);

                                                        this.savedItemsCollection = ReadingList;
                                                        this.recommendedCollection = new Recommendations(); // Don't use shared instance, because we're modifying the location parameter

                                                        // Hard-code production environment for the duration of the test
                                                        this.recommendedCollection.url = 'https://rec.api.nytimes.com/svc/recommendations/v4/personalized.json';
                                                        if (this.recommendedCollection.setLocation) {
                                                            this.recommendedCollection.setLocation('hprl');
                                                        }
                                                    },

                                                    handleUserReady: function() {
                                                        var hasAbra = window.NYTD && window.NYTD.Abra;

                                                        if (hasAbra) {
                                                            this.variant = window.NYTD.Abra('www-readlater-test-1');
                                                        } else {
                                                            this.variant = 'control';
                                                        }

                                                        if (this.variant === 'variant1' || this.variant === 'variant2') {
                                                            if (userData.isLoggedIn()) {
                                                                this.subscribe(this.savedItemsCollection, 'nyt:xps-articles-loaded', this.savedArticlesRefreshed);
                                                                this.subscribe(this.savedItemsCollection, 'nyt:xps-article-removed', this.articleRemoved);
                                                                this.subscribe(this.savedItemsCollection, 'nyt:xps-saved', this.articleSaved);

                                                                this.syncData();
                                                            } else {
                                                                this.totalSavedItems = 0;
                                                                this.prepareForRefresh();
                                                                this.subscribeOnce(this.recommendedCollection, 'sync', this.recommendedStoriesRefreshed);
                                                                this.recommendedCollection.loadData();
                                                            }
                                                        }
                                                    },

                                                    syncData: function() {
                                                        this.savedItemsCollection.getSavedArticles();
                                                    },

                                                    articleRemoved: function(url) {
                                                        var $li = this.$el.find('a[href="' + url + '"]').closest('li');

                                                        this.totalSavedItems = this.totalSavedItems - 1;
                                                        this.$el.find('.story-count a').html(this.totalSavedItems);

                                                        if (this.isArticleInReadingList(url)) {
                                                            $li.find('.save-action').addClass('unsaved').removeClass('saved');
                                                            $li.find('.freshness').html('Removed from your Reading List');
                                                        } // Nothing to do in case where unsaved article is not in list
                                                    },

                                                    articleSaved: function(url) {
                                                        var $li = this.$el.find('a[href="' + url + '"]').closest('li');

                                                        this.totalSavedItems = this.totalSavedItems + 1;
                                                        this.$el.find('.story-count a').html(this.totalSavedItems);

                                                        if (this.isArticleInReadingList(url)) {
                                                            $li.find('.save-action').addClass('saved').removeClass('unsaved');
                                                            $li.find('.freshness').html('Added today');
                                                        } else {
                                                            this.syncData(); // hard-reload entire module
                                                        }
                                                    },

                                                    isArticleInReadingList: function(url) {
                                                        var readingListUrls = this.$el.find('.story .headline a').map(function(i, anchor) {
                                                            return anchor.getAttribute('href');
                                                        });

                                                        return _.contains(readingListUrls, url);
                                                    },

                                                    prepareForRefresh: function() {
                                                        if (this.renderableSavedStories.length >= 2) {
                                                            this.$el.find('.about-recommendations').addClass('disabled');
                                                            this.$el.find('.reading-list-link').removeClass('disabled');
                                                            this.$el.find('.about-reading-list').removeClass('disabled');
                                                        }

                                                        this.$el.find('.story-count a').html(this.totalSavedItems);
                                                        this.$el.find('.saved-stories').empty();
                                                        this.$el.find('.recommended-stories').empty();
                                                    },

                                                    savedArticlesRefreshed: function() {
                                                        this.totalSavedItems = this.savedItemsCollection.totalCount;

                                                        // Filter out saved watching items and items without headline/image
                                                        this.renderableSavedStories = _.first(this.savedItemsCollection.select(
                                                            function(item) {
                                                                var data = item.get('Data');
                                                                var isWatching = data && data.Type === 'watchingrecommendation';
                                                                return data && !isWatching && data.Headline && data.ImageCrops;
                                                            }), 3);

                                                        this.prepareForRefresh();
                                                        this.appendSavedStories();

                                                        if (this.renderableSavedStories.length >= 3) {
                                                            this.$el.removeClass('disabled');
                                                            this.setupImpressionTracking();
                                                        } else {
                                                            this.$el.find('.saved-stories').removeClass('has-last-item');
                                                            this.$el.find('.recommended-stories').addClass('has-last-item');

                                                            if (this.recommendedCollection.hasFetched) {
                                                                this.appendRecommendedStories();
                                                            } else {
                                                                this.subscribeOnce(this.recommendedCollection, 'sync', this.recommendedStoriesRefreshed);
                                                                this.recommendedCollection.loadData();
                                                            }
                                                        }
                                                    },

                                                    recommendedStoriesRefreshed: function() {
                                                        this.appendRecommendedStories();
                                                        this.$el.removeClass('disabled');
                                                        this.setupImpressionTracking();
                                                    },

                                                    appendSavedStories: function() {
                                                        var i, article, headline, href, imgSrc, addedAt, $li;
                                                        for (i = 0; i < this.renderableSavedStories.length; i++) {
                                                            article = this.renderableSavedStories[i];
                                                            if (!article) {
                                                                continue;
                                                            }

                                                            headline = article.get('Data').Headline;
                                                            href = article.get('URL');
                                                            imgSrc = article.get('Data').ImageCrops['ThumbLarge'];
                                                            addedAt = new Date(article.get('UpdatedDate'));

                                                            $li = $('<li class="story-container"></li>');
                                                            $li.append('<article class="story"><a href="javascript:;" class="save-action saved"><i class="icon"></i><span class="text">Remove Saved Item</span></a><div class="story-body"><h3 class="headline"><a href="' + href + '">' + headline + '</a></h3><div class="freshness">' + this.formattedAddedAtString(addedAt) + '</div></div></article>');
                                                            if (imgSrc.length > 0) {
                                                                $li.find('article').append('<figure class="thumbnail"><a href="' + href + '"><img alt="" src="' + imgSrc + '"></a></figure>');
                                                            }
                                                            this.$el.find('.saved-stories').append($li);
                                                        }
                                                    },

                                                    appendRecommendedStories: function() {
                                                        var storyCount = 3 - this.renderableSavedStories.length;
                                                        var i, article, headline, href, promo, thumbStandard, imgSrc, $li;

                                                        for (i = 0; i < storyCount; i++) {
                                                            article = this.recommendedCollection.at(i);
                                                            headline = article.get('headline');
                                                            href = article.get('link');
                                                            promo = article.get('promotional_media');
                                                            thumbStandard = promo && promo.image && promo.image.image_crops && promo.image.image_crops.thumbStandard;
                                                            imgSrc = thumbStandard && thumbStandard.url;

                                                            $li = $('<li class="story-container"></li>');
                                                            $li.append('<article class="story"><a href="javascript:;" class="save-action unsaved"><i class="icon"></i><span class="text">Save</span></a><div class="story-body"><h3 class="headline"><a href="' + href + '">' + headline + '</a></h3><div class="freshness">Recommended for you</div></div></article>');
                                                            if (imgSrc) {
                                                                $li.find('article').append('<figure class="thumbnail"><a href="' + href + '"><img alt="" src="' + imgSrc + '"></a></figure>');
                                                            }
                                                            this.$el.find('.recommended-stories').append($li);
                                                        }
                                                    },

                                                    handleLinkClick: function(ev) {
                                                        var href;
                                                        var $el = $(ev.currentTarget);

                                                        if ($el.hasClass('save-action')) {
                                                            return;
                                                        }

                                                        href = this.trackingAppendParams($el.attr('href'), {
                                                            'action': 'click',
                                                            'pgtype': 'Homepage',
                                                            'module': 'readinglist',
                                                            'version': 'readinglist',
                                                            'region': 'c-column-middle-span-region'
                                                        });

                                                        $el.attr('href', href);
                                                    },

                                                    handleSaveToggleClick: function(ev) {
                                                        var href;
                                                        var $el = $(ev.currentTarget);

                                                        if (userData.isLoggedIn()) {
                                                            href = $el.closest('.story').find('.headline a').attr('href');
                                                            if ($el.hasClass('unsaved')) {
                                                                this.recordTrackingInteraction('save-article', href);
                                                                this.savedItemsCollection.save(href);
                                                            } else {
                                                                this.recordTrackingInteraction('remove-article', href);
                                                                this.savedItemsCollection.removeSavedArticle(href);
                                                            }
                                                        } else {
                                                            ev.stopPropagation();
                                                            this.recordTrackingInteraction('save-to-login', href);
                                                            this.broadcast('nyt:loginmodal-open', {
                                                                modalTitle: 'Log in to Save',
                                                                moduleName: 'Save'
                                                            });
                                                        }
                                                    },

                                                    formattedAddedAtString: function(date) {
                                                        var addedAtString = 'Added ';
                                                        var today = new Date();

                                                        if (date.getDate() == today.getDate() && date.getMonth() == today.getMonth() && date.getFullYear() == today.getFullYear()) {
                                                            addedAtString += 'today';
                                                        } else {
                                                            addedAtString += this.dateHelper.getMonthShortName(date.getMonth()) + ' ' + date.getDate() + ', ' + date.getFullYear();
                                                        }

                                                        return addedAtString;
                                                    },

                                                    _setupImpressionTracking: function() {
                                                        if (this.pageManager.isComponentVisible(this.$el)) {
                                                            this.recordTrackingImpression();
                                                        } else {
                                                            this.subscribe('nyt:page-scroll', this.handleScroll);
                                                        }
                                                    },

                                                    handleScroll: function() {
                                                        if (this.pageManager.isComponentVisible(this.$el)) {
                                                            this.recordTrackingImpression();
                                                            this.stopSubscribing('nyt:page-scroll');
                                                        }
                                                    },

                                                    recordTrackingImpression: function() {
                                                        this.trackingTriggerImpression('readinglist-impression', {
                                                            'module': 'readinglist',
                                                            'pgtype': 'Homepage',
                                                            'region': 'c-column-middle-span-region',
                                                            'action': 'impression',
                                                            'version': this.totalSavedItems.toString(),
                                                            'proxyEventType': 'impression',
                                                            'timestamp': new Date().getTime()
                                                        });
                                                    },

                                                    recordTrackingInteraction: function(eventName, url) {
                                                        this.trackingTrigger(eventName, {
                                                            'module': 'readinglist',
                                                            'eventName': eventName,
                                                            'pgtype': 'Homepage',
                                                            'region': 'c-column-middle-span-region',
                                                            'action': 'click',
                                                            'version': url,
                                                            'proxyEventType': 'action',
                                                            'timestamp': new Date().getTime()
                                                        });
                                                    }
                                                });

                                                new MyStoriesView();
                                            });
                                    });
                                </script>
